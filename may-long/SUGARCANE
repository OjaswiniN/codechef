#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
	  int n;
	  cin>>n;
	 int amount =n*50;
	 int sugarprice = (amount*20/100);
	 int saltprice = (amount*20/100);
	 int mintprice= (amount*30/100);
	 int profit = amount-(sugarprice+saltprice+mintprice);
	 cout<< profit<<endl;
	}
	
	return 0;
}
